				<li class="dropdown"><a aria-expanded="false" aria-haspopup="true"
					role="button" data-toggle="dropdown" class="dropdown-toggle"
					href="#">{{ 'navbar.phototheque'|trans }}<span class="caret"></span></a>
					<ul class="dropdown-menu">
						<li><a href="{{ url('map') }}" class="menu-map">{{ 'navbar.map.title'|trans }}</a></li>
						<li><a href="{{ url('keywords') }}" class="menu-library">{{ 'navbar.themas.title'|trans }}</a></li>
						<li><a href="{{ url('homepage') }}#services" class="menu-services">{{ 'navbar.free_search.title'|trans }}</a></li>
					</ul></li>
				<li class="dropdown"><a aria-expanded="false" aria-haspopup="true"
					role="button" data-toggle="dropdown"
					class="dropdown-toggle menu-technics" data="data-sroll" href="#">{{ 'navbar.technics.title'|trans }}<span
						class="caret"></span></a>
					<ul class="dropdown-menu">
						<li><a href="{{ url('homepage') }}#technics" class="technicsOne">{{ 'navbar.photographer.title'|trans }}</a></li>
						<li><a href="{{ url('homepage') }}#technics" class="technicsTwo">{{ 'navbar.helico.title'|trans }}</a></li>
